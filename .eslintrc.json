{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-globals":0,
        "no-underscore-dangle":0,
        "no-new":0,
        "class-methods-use-this":0,
        "no-console":0,
        "consistent-return":0,
        "no-prototype-builtins":0
    }
}
